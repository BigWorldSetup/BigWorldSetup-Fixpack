                     ("%target%" = "%eff_target%") AND
                     ("%power%" = "%eff_power%") AND
                     ("%param1%" = "%eff_param1%") AND
                     ("%param2%" = "%eff_param2%") AND
                     ("%timing%" = "%eff_timing%") AND
                     ("%dispel%" = "%eff_dispel%") AND
                     ("%duration%" = "%eff_duration%") AND
                     ("%prob1%" = "%eff_prob1%") AND
