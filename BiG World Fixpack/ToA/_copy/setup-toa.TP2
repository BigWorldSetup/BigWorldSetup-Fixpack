BEGIN @392
REQUIRE_PREDICATE !GAME_IS ~bg2ee eet~ @394
DESIGNATED 05 GROUP @10002

OUTER_PATCH_SAVE orig_bytes_1 ~~ BEGIN
    INSERT_BYTES 0x000 0x9
    WRITE_LONG   0x000 0xdc82c766
    WRITE_LONG   0x004 0x80000005
    WRITE_BYTE   0x008 0x00
END

OUTER_PATCH_SAVE patch_bytes_1 ~~ BEGIN
    INSERT_BYTES 0x000 0x9
    WRITE_LONG   0x000 0xdc82c766
    WRITE_LONG   0x004 0xe0000005
    WRITE_BYTE   0x008 0x00
END

COPY bgmain.exe bgmain.exe
    READ_ASCII 0x002739d2 target_bytes_1 ELSE 0 (0x9)
    PATCH_IF (~%target_bytes_1%~ STRING_EQUAL ~%orig_bytes_1%~)
    THEN BEGIN
        WRITE_ASCIIE 0x002739d2 ~%patch_bytes_1%~ (0x9)
        INNER_ACTION BEGIN
            COPY ~ToA/base/toolscrl.bam~ ~override~
        END
    END ELSE
    PATCH_IF (~%target_bytes_1%~ STRING_EQUAL ~%patch_bytes_1%~)
    THEN BEGIN
        PATCH_PRINT ~Patch already applied. Skipping ...~
    END ELSE INNER_ACTION BEGIN FAIL ~Target bytes don't match. Aborting ...~ END
BUT_ONLY




