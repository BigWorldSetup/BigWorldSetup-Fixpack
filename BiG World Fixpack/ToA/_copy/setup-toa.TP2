BACKUP ~ToA/Backup~
AUTHOR ~whiteagnus@baldursgatemods.de~
//Original Author: Naurandir
VERSION ~v2.4_20160104~

README ~ToA/readme.%LANGUAGE%.txt~

AUTO_TRA "ToA/Language/%s/D"//%"

ALWAYS
	INCLUDE ~ToA/lib/a7_tools.tpa~
	INCLUDE ~ToA/lib/EET_functions.tph~
	
	ACTION_DEFINE_ARRAY charsetsReloadArray BEGIN setup journal END
	ACTION_DEFINE_ARRAY charsetsNoConvertArray BEGIN END
		LAF ~HANDLE_CHARSETS~
			INT_VAR
			infer_charsets = 1
			STR_VAR
			tra_path = "ToA/language"
			iconv_path = "ToA/tools/win32/iconv"
			charset_table = charsetsTable
			reload_array = charsetsReloadArray
			noconvert_array = charsetsNoConvertArray
	END
	LOAD_TRA ~ToA/language/%LANGUAGE%/setup.tra~
	
	ACTION_IF GAME_IS ~bg2ee eet~ BEGIN
		OUTER_SPRINT ~res~ ~ee~
	END ELSE ACTION_IF GAME_IS ~bgt tob~ BEGIN
		OUTER_SPRINT ~res~ ~normal~
	END
END

LANGUAGE ~Deutsch (German)~
         ~German~
         ~ToA/language/german/setup.tra~
         ~ToA/language/german/journal.tra~

LANGUAGE ~English by Headbanger~
         ~English~
         ~ToA/language/english/setup.tra~
         ~ToA/language/english/journal.tra~

LANGUAGE ~Italian by ilot~
         ~italian~
         ~ToA/language/italian/setup.tra~
         ~ToA/language/italian/journal.tra~

LANGUAGE ~Polski (Polish) by Aristo~
         ~polish~
         ~ToA/language/polish/setup.tra~
         ~ToA/language/polish/journal.tra~
		 
BEGIN @1 DESIGNATED 01
REQUIRE_PREDICATE GAME_IS ~bgt eet tob bg2ee~ @393

ACTION_DEFINE_ARRAY arrayName BEGIN ~ToA/base/baf/ZPALA3.baf~ ~ToA/base/baf/area/AR0700.baf~ ~ToA/base/baf/area/AR5500.baf~ END
LAF ~EET_JOURNAL~
	INT_VAR
	journal_game = 0 //0 - BG2 (+12); 1 - SoD (+7) (default = 0)
	journal_patch_mode = 0 //0 - replace with values; 1 - replace with variables (default = 0)
	STR_VAR
	journal_array = EVAL ~arrayName~ //array with files to patch (supports: d, dlg, baf, bcs, and any text file; if path points to override COPY_EXISTING is used)
END

